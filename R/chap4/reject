u<-runif(1000)
hist(u)
n<-rnorm(1000)
hist(n)

p<-c(0.11,0.12,0.09,0.08,0.12,0.10,0.09,0.09,0.10,0.10)
U1<-runif(100)
Y<-floor(10*U1)+1
U2<-runif(100)
N<-(sign(p[Y]/0.12-U2)+1)/2
sum(N)
X<-N*Y
X[X>0]

U1<-runif(100)
U2<-runif(100)
N<-(sign(U1*(1-U1)^3*256/27-U2)+1)/2
sum(N)
X<-N*U1
X[X>0]

